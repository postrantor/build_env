{
  "names": {
    "beatles": {
      "cmake-args": [
        "-DCMAKE_C_COMPILER=clang",
        "-DCMAKE_CXX_COMPILER=clang++"
      ],
      "dependencies": []
    },
    "cream": {
      "cmake-args": [
        "-DCMAKE_C_COMPILER=clang",
        "-DCMAKE_CXX_COMPILER=clang++",
        "-DBUILD_TESTING=OFF"
      ],
      "dependencies": [
        "protobuf",
        "cyclonedds-cxx"
      ]
    },
    "CycloneDDS-CXX": {
      "cmake-args": [],
      "dependencies": [
        "cyclonedds"
      ]
    },
    "quadruped": {
      "cmake-args": [
        "-DUSE_AVX2=OFF"
      ]
    },
    "SuiteSparse": {
      "cmake-args": [
        "-DSUITESPARSE_ENABLE_PROJECTS=amd",
        "-DSUITESPARSE_CONFIG_USE_OPENMP=OFF"
      ]
    },
    "protobuf": {
      "cmake-args": [
        "-Dprotobuf_BUILD_TESTS=OFF",
        "-DBUILD_SHARED_LIBS=ON",
        "-Dprotobuf_BUILD_LIBPROTOC=ON",
        "-Dprotobuf_BUILD_EXAMPLES=ON"
      ]
    },
    "spdlog_vendor": {
      "cmake-args": [
        "-DFORCE_BUILD_VENDOR_PKG=ON"
      ]
    },
    "yaml_cpp_vendor": {
      "cmake-args": [
        "-DFORCE_BUILD_VENDOR_PKG=ON"
      ]
    },
    "qpOASES": {
      "cmake-args": [
        "-DQPOASES_BUILD_EXAMPLES=OFF"
      ]
    },
    "cyber_rt": {
      "cmake-args": [
        "-DCMAKE_C_COMPILER=gcc",
        "-DCMAKE_CXX_COMPILER=g++"
      ],
      "dependencies": [
        "fastrtps",
        "nlohmann_json",
        "protobuf",
        "gflags",
        "glog",
        "googletest",
        "tinyxml2"
      ]
    },
    "fastrtps": {
      "cmake-args": [
        "-DTHIRDPARTY=ON",
        "-DEPROSIMA_BUILD=OFF",
        "-DCMAKE_BUILD_TYPE=Release",
        "-DEPROSIMA_INSTALLER=OFF",
        "-DCOMPILE_EXAMPLES=OFF",
        "-DCMAKE_C_COMPILER=gcc",
        "-DCMAKE_CXX_COMPILER=g++"
      ]
    },
    "nlohmann_json": {
      "cmake-args": []
    },
    "tinyxml2": {
      "cmake-args": []
    },
    "gflags": {
      "cmake-args": [
        "-DCMAKE_CXX_FLAGS=-fPIC",
        "-DBUILD_SHARED_LIBS=ON",
        "-DWITH_GFLAGS=ON"
      ]
    },
    "glog": {
      "cmake-args": [
        "-DCMAKE_CXX_FLAGS=-fPIC",
        "-DBUILD_SHARED_LIBS=ON",
        "-DCMAKE_BUILD_TYPE=OFF"
      ]
    },
    "googletest-distribution": {
      "cmake-args": [
        "-DCMAKE_CXX_FLAGS=-Wno-error=maybe-uninitialized"
      ]
    },
    "googletest": {
      "cmake-args": [
        "-DCMAKE_CXX_FLAGS=-fPIC",
        "-DBUILD_SHARED_LIBS=ON"
      ]
    }
  }
}
